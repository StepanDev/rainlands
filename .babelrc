{
  "presets": ["stage-2"],
  "plugins": [
    "transform-class-properties",
    "transform-export-extensions",
    [
      "module-resolver",
      {
        "alias": {
          "@src": "./src",
          "@ui": "./src/ui",
          "@client": "./src/client",
          "@server": "./src/server",
          "@packages": "./src/packages"
        }
      }
    ]
  ]
}
