{
  "extends": ["airbnb-base", "@atomix/eslint-config"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "env": {
    "browser": true,
    "node": true
  },
  "plugins": ["prettier"],
  "rules": {
    "no-underscore-dangle": 0,
    "import/extensions": 0,
    "import/no-unresolved": 0
  }
}
